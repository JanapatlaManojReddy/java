package oops;

public class Constructors {

	public static void main(String[] args) {
		Am ref= new Am();
		Am ref1= new Am(1222,"vinay",false);
		ref.dis();
		
		

	}

}
class Am{
	int a;
	String s;
	boolean c;
	Am(){//default constructor
		a=122;
		s="manoj";
		c=true;
		System.out.println(a+s+c);
	}
	Am(int a,String s,boolean c){
		this.a=a;
		this.s=s;
		this.c=c;
		System.out.println(a+s+c);
		
	}
	void dis() {
		System.out.println(a+s+c);
	}
	
}
