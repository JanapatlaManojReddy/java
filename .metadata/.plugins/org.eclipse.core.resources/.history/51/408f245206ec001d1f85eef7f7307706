package basics;

public class Operators{
	static int a = 5;
	static int b = 10;
	public static void main(String[] args) {
		// +,-,/,%
		int c=0;
		
		
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a/b);
		System.out.println(a%b);
		
		System.out.println(b+a);
		System.out.println(b-a);
		System.out.println(b/a);
		System.out.println(b%a);
		System.out.println(b=a---a);
		System.out.println(++a);
		System.out.println(b);

	}

}
