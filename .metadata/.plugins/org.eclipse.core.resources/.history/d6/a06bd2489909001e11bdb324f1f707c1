package oops;

public class Constructors {

	public static void main(String[] args) {
		
		Am ref= new Am();
		ref.dis();
		
		

	}

}
class Am{
	int a;
	String s;
	boolean c;
	Am(){//default constructor
		this(122,"manoj",true);
		a=122;
		s="manoj";
		c=false;
		System.out.println(a+s+c);
		
	}
	Am(int a,String s,boolean c){
		
		this.a=a;
		this.s=s;
		this.c=c;
		System.out.println(a+s+c);
		
	}
	void dis() {
		System.out.println(a+s+c);
	}
	
}
